<div class="error">
  <% if flash[:alert] %>
    <p class="errors"><%= flash[:alert] %></p>
  <% end %>
</div>

<div class="standard-body-outer">
  <h1>Welcome to Stack Told You So!</h1>
  <div class="standard-body-inner">
    <%@questions.each do |question|%>
    <%= render partial:'link', layout:false, locals: {question: question} %>
  <%end%>
  </div>
</div>
